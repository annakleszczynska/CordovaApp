{
  "app": {
    "base_language": "en", 
    "pull": {
      "target": [
        {
            "tag":"SystemStrings",
            "file_format": "nested_json", 
            "path": "./translations/app/<language>.json"
        },
        {
            "tag":"AppStrings",
            "file_format": "node_2_json", 
            "path": "./www/i18n/locale-<language>.json"  
        }    
	  ]
    }, 
    "push": {
      "source": [
        {
            "tag":"SystemStrings",
            "file_format": "nested_json", 
            "path": "./translations/app/<language>.json"
        },
        {
            "tag":"AppStrings",
            "file_format": "node_2_json", 
            "path": "./www/i18n/locale-<language>.json"  
		}	
      ]
    }
  }
}
